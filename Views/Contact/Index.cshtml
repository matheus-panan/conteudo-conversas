@model List<string>

<h2>IDs dos Contatos</h2>

@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered">
        <thead>
        <tr>
            <th>ID</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var id in Model)
        {
            <tr>
                <td>@id</td>
            </tr>
        }
        </tbody>
    </table>
}
else
{
    <p>Nenhum ID encontrado.</p>
}